{
    "compilerOptions": {
        "target": "ES2018",
        "module": "ESNext",
        "moduleResolution": "Node",
        "lib": [
            "ESNext",
            "ESNext.AsyncIterable",
            "DOM"
        ],
        "esModuleInterop": true,
        "allowJs": true,
        "sourceMap": true,
        "strict": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
        "~/*": [
            "./*"
        ],
        "@/*": [
            "./*"
        ]
        },
        "types": [
            "@types/node",
            "@nuxt/types"
        ],
        "typeRoots": [
            "./types",
            "./node_modules/@types"
        ],
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules"
    ]
}